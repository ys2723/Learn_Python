# For Loop

countries = ['Burundi','Oman', 'Zambia', 'Burkina Faso', 'Togo']
for item in countries:
    print(item) 
    
    
print("\n")
for i in range(10): #prints 0 to 9
    print(i)
    
print("\n")
for t in range(1,10): #prints 1 to 9
    print(t)
    
print("\n")
for s in range(1,10,2): #every second is printed between 1 and 10
    print(s)
